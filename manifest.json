{
    "manifest_version": 3,
    "name": "Noteman",
    "version": "1.0.0",
    "description": "Full page mad simple note-taking app",
    "author": "Alexander Bridgeman",
    "icons": {
        "16": "images/treeicon.png",
        "48": "images/treeicon.png",
        "128": "images/treeicon.png"
      },
    "background": {
        "service_worker": "serviceworker.js"
    },
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_title": "Open Noteman"
    }
}
